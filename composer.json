{
  "name": "browserstack/phpunit-browserstack",
  "description": "TestCase wrapper to assist with browserstack.com",
  "keywords": ["selenium", "php", "browserstack", "local"],
  "homepage": "https://github.com/browserstack/phpunit-browserstack",
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/browserstack/phpunit-browserstack/issues",
    "source": "https://github.com/browserstack/phpunit-browserstack"
  },
  "require": {
    "browserstack/browserstack-local": "dev-master",
    "phpunit/phpunit-selenium": "*",
    "facebook/webdriver": "dev-master"
  },
  "scripts": {
    "test": "composer single && composer local && composer parallel",
    "single": "vendor/bin/phpunit tests/single_test.php",
    "local": "CONFIG_FILE=config/local.conf.json vendor/bin/phpunit tests/local_test.php",
    "parallel": "CONFIG_FILE=config/parallel.conf.json /usr/bin/env php lib/parallel.php"
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserStack\\": "lib/" 
    }
  }
}
